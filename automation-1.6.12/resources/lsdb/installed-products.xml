<?xml version="1.0" encoding="UTF-8"?> 

<lsd-install release="1.0">     
  <!-- This is a short identifier of the installation name.  If the installation is going to be registered on the Grid, this should match with the short name in the POC info. -->     
  <location>Production</location>     
  <applications>         
    <application>             
      <!-- One of: caArray, caGWAS, CTODS, NCIA, caTissue -->             
      <name>caArray</name>             
      <version>2.0.2</version>             
      <!-- URLs (or worst case, hostname + port) for the web interface homepage and the grid data service -->             
      <url type="grid">http://array.nci.nih.gov:80/wsrf/services/cagrid/CaArraySvc</url>             
      <url type="application">http://array.nci.nih.gov</url>
    </application>            
    <application>             
      <name>NCIA</name>             
      <version>3.7</version>             
      <url type="grid">http://imaging4-qa.nci.nih.gov/wsrf/services/cagrid/NCIACoreService</url>             
      <url type="application">http://imaging.nci.nih.gov</url>
	</application>         
    <application>             
      <name>caTissue</name>             
      <version>1.0</version>             
      <url type="grid">https://130.132.232.64:8443/wsrf/services/cagrid/CaTissueCore</url>
	</application>
  </applications>
</lsd-install>