<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="gov.nih.nci.bda.certification.domain.TargetLookup" table="TARGET_LOOKUP">
         <id name="id" column="id"> 
         	<generator class="increment"/>           
         </id>
         <property name="targetName" column="TARGET_NAME"/>
         <property name="mapName" column="MAP_NAME"/>
         <property name="isValue" column="IS_VALUE"/>
         <property name="isOptional" column="IS_OPTIONAL"/>
        <property name="saveProperties" column="SAVE_PROPERTIES" />
        <property name="useProperties" column="USE_PROPERTIES" />
     </class>	
</hibernate-mapping>
