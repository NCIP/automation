				<jboss-verify-keystore phase="1" prefix="" disable="false" override="false" pre-run="false" post-run="false">
					<!-- properties for basedir, build.file.location and build target could be specified here-->
				</jboss-verify-keystore>
				<!-- plugin -->
				<!-- stop jboss  -->
				<jboss-stop phase="4" prefix="" disable="false" override="false" pre-run="false" post-run="false">
					<!-- properties for basedir, build.file.location and build target could be specified here-->
