<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="gov.nih.nci.bda.monitor.domain.ProjectBuildStatus" table="project_build_status">
         <id name="id" column="id"> 
         	<generator class="increment"/>           
         </id>
         <property name="product" column="product"/>
         <property name="dev" column="dev"/>
         <property name="qa" column="qa"/>
         <property name="stage" column="stage"/>
         <property name="prod" column="prod"/>
     </class>	
</hibernate-mapping>
